{
  "name": "website-filter",
  "private": true,
  "version": "0.0.0",
  "description": "A Chrome Extension for Filter Website Contents",
  "scripts": {
    "dev": "vite",
    "build": "npm run build-main && npm run build-cs",
    "build-main": "vue-tsc --noEmit && vite build",
    "build-cs": "vue-tsc --noEmit && vite build --config=vite-content-script.config.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "element-plus": "^2.1.5",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.2.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.6",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  }
}
